<div>
    <h1>Hola {{user.name}}! Bienvenido</h1>
    {{#if isAdmin}}
        {{#each users}}
            <div>
                <p>Nombre: {{this.name}} - email: {{this.email}}</p>
                <hr>
            </div>
        {{/each}}
    {{else}}
        <h2>No tienes permisos para ver los usuarios</h2>
    {{/if}}
</div>
<script src="/static/assets/js/index.js"></script>